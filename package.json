{
  "name": "snitch",
  "version": "2.0.0",
  "description": "Front-end error logging and event tracking.",
  "main": "lib/snitch.js",
  "repository": "git@github.com:JamieWohletz/snitch.git",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint --fix .",
    "build:dev": "cross-env NODE_ENV=dev webpack",
    "build:dev:watch": "cross-env NODE_ENV=dev webpack --progress --colors --watch",
    "build:demo": "cross-env NODE_ENV=demo webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "dist": "npm run build:dev && npm run build:demo && npm run build:prod",
    "test": "./node_modules/.bin/karma start karma.config.js"
  },
  "keywords": [
    "error",
    "tracking",
    "logging",
    "browser"
  ],
  "author": "Jamie Wohletz",
  "license": "MIT",
  "dependencies": {
    "unique-selector": "^0.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "mocha": "^3.1.2",
    "uglify-save-license": "^0.4.1",
    "webpack": "^1.13.3"
  }
}
